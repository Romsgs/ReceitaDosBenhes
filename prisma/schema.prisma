// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Receita {
  id Int @id @default(autoincrement())
  path String
  nome String
  autor String
}

model Usuarios {
  id Int @id @default(autoincrement())
  nickname String @unique
  email String @unique
  senha String
}